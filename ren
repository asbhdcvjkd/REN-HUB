-- Scripts:

local function XWFFNQ_fake_script() -- Scanner.ScannerClickHandler 
	local script = Instance.new('LocalScript', Scanner)

	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local StarterGui = game:GetService("StarterGui")
	
	local scannerButton = script.Parent
	local screenGui = scannerButton:FindFirstAncestorOfClass("ScreenGui")
	
	if not screenGui then
		warn("REN Backdoor: Parent ScreenGui not found for button:", scannerButton:GetFullName())
	    return
	end
	
	local scanInterval = 0.5 -- Time in seconds between logging each service scan
	
	scannerButton.MouseButton1Click:Connect(function()
	    scannerButton.Text = "Searching..." -- Update button text at the start of the scan
		print("REN Backdoor: Initiating scan...")
	    local startTime = os.clock()
	
		print("REN Backdoor: Searching for Backdoor in ReplicatedStorage...")
	    local backdoorEvent = ReplicatedStorage:FindFirstChild("Backdoor")--name of the backdoor
	    local timeTakenForInitialSearch = os.clock() - startTime
	
	    if backdoorEvent then
	        local totalTimeTaken = os.clock() - startTime -- Total time until found
	
	        print("Backdoor found in ReplicatedStorage in " .. string.format("%.4f", timeTakenForInitialSearch) .. " seconds.")
	        
	        StarterGui:SetCore("SendNotification", {
	            Title = "REN hub Backdoor",
	            Text = "Found Backdoor in " .. string.format("%.4f", totalTimeTaken) .. " seconds.",
	            Duration = 5 -- Duration in seconds
	        })
	
	        local neoHubGui = screenGui:FindFirstChild("Neo hub gui")
	        if neoHubGui and neoHubGui:IsA("Frame") then
	            neoHubGui.Visible = false
	        else
				warn("REN Backdoor: 'Neo hub gui' Frame not found or not a Frame in", screenGui:GetFullName())
	        end
	
	        local readyToHubGui = screenGui:FindFirstChild("Ready to hub")
	        if readyToHubGui and readyToHubGui:IsA("Frame") then
	            readyToHubGui.Visible = true
	            if neoHubGui and neoHubGui:IsA("Frame") then -- Ensure neoHubGui is also valid before accessing its position
	                readyToHubGui.Position = neoHubGui.Position
	            end
	        else
				warn("REN Backdoor: 'Ready to hub' Frame not found or not a Frame in", screenGui:GetFullName(), ". Please ensure it exists.")
	        end
	        -- If backdoor is found, the button text might be updated here if needed.
	        -- For now, it might stay "Searching..." or be handled by UI flow.
	        -- If backdoor is found, we can set text to "Backdoor Found!" or similar.
	        -- scannerButton.Text = "Backdoor Found!" 
	    else
			print("REN Backdoor: 'REN Backdoor' not found in ReplicatedStorage after " .. string.format("%.4f", timeTakenForInitialSearch) .. " seconds.")
	        print("REN Backdoor: Initiating extended scan...")
	
	        local servicesToScan = {
	            "Workspace", 
				"Lighting", 
	            "ReplicatedStorage", 
				"ServerScriptService", 
				"ServerStorage", 
	            "StarterGui", 
	            "StarterPlayer",
	            "SoundService",
	            "Chat",
	            "Players",
	            "Teams"
	        }
	        
	        local skipDelay = false -- Flag to control if the delay should be skipped
	
	        for _, serviceName in servicesToScan do
				print("REN Backdoor: Searching for Backdoor in " .. serviceName .. "...")
	            
	            if not skipDelay then
	                task.wait(scanInterval) -- Wait for the configured interval
	                if serviceName == "ReplicatedStorage" then
	                    skipDelay = true -- After ReplicatedStorage is scanned (and waited for), skip delays for subsequent services
	                end
	            end
	            -- We don't actually search here, just log the attempt for simulation
	        end
	        
	        local totalTimeTaken = os.clock() - startTime -- Total time from the very start
	
			print("REN Backdoor: scan complete. 'REN Backdoor' still not found. Total scan time: " .. string.format("%.4f", totalTimeTaken) .. " seconds.")
	        
	        StarterGui:SetCore("SendNotification", {
	            Title = "REN hub Backdoor",
	            Text = "'REN Backdoor' not found after searching multiple services. Time: " .. string.format("%.4f", totalTimeTaken) .. "s.",
	            Duration = 7 -- Longer duration for this notification
	        })
	        scannerButton.Text = "No Backdoor :("
	    end
	end)
	
	
end
coroutine.wrap(XWFFNQ_fake_script)()
local function HBQAG_fake_script() -- Neohubgui.UIDrag 
	local script = Instance.new('LocalScript', Neohubgui)

	-- Made by Real_IceyDev (@lceyDex) --
	-- Simple UI dragger (PC Only/Any device that has a mouse) --
	
	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
end
coroutine.wrap(HBQAG_fake_script)()
