local function CBNA_fake_script() -- R6.LocalScript 
	local script = Instance.new('LocalScript', R6)

	
	script.Parent.MouseButton1Click:connect(function() -- ClientHandler script (local script)
		script.ServerEvent:FireServer()
	end)
end
