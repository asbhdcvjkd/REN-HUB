local function XKUY_fake_script() -- RE.Script 
	local script = Instance.new('Script', RE)

	script.Parent.RemoteEvent.OnServerEvent:Connect(function(plr)
		local CFrame = plr.Character:FindFirstChild("HumanoidRootPart").CFrame
		plr:LoadCharacter()	
		local HRP = workspace:WaitForChild(plr.Name):WaitForChild("HumanoidRootPart")
		HRP.CFrame = CFrame
	end)
end
coroutine.wrap(XKUY_fake_script)()
local function OCOOQK_fake_script() -- RE.LocalScript 
	local script = Instance.new('LocalScript', RE)

	local plr = game:GetService("Players").LocalPlayer
	
	script.Parent.MouseButton1Click:Connect(function()
		local CFrame = plr.Character:FindFirstChild("HumanoidRootPart").CFrame
		local CameraCFrame = workspace.Camera.CFrame
		script.Parent.RemoteEvent:FireServer()
		local HRP = workspace:WaitForChild(plr.Name):WaitForChild("HumanoidRootPart")
		HRP.CFrame = CFrame
		workspace.Camera.CFrame = CameraCFrame
		workspace.Camera.Changed:Wait()
		workspace.Camera.CFrame = CameraCFrame
	end)
end
coroutine.wrap(OCOOQK_fake_script)()
